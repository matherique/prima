{
  "name": "prima",
  "version": "1.0.0",
  "description": "learning prisma ",
  "main": "index.js",
  "scripts": {
    "predev": "make -s generate",
    "dev": "npx nodemon --exec ts-node src/index.ts",
    "prisma:generate": "make -s generate && npx prisma generate --schema src/prisma/schema.prisma",
    "prisma:format": "make -s generate && npx prisma format --schema src/prisma/schema.prisma",
    "test": "jest",
    "test:watch": "jest --watch --bail --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matherique/prima.git"
  },
  "author": "Matheus Henrique",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matherique/prima/issues"
  },
  "homepage": "https://github.com/matherique/prima#readme",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.10",
    "dotenv": "^10.0.0",
    "jest": "^27.1.0",
    "jest-mock-extended": "^2.0.2-beta2",
    "nodemon": "^2.0.12",
    "prisma": "^2.30.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@prisma/client": "^2.30.2"
  }
}
